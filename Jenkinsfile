#!/usr/bin/env groovy

pipeline{
  agent any
  
  stages{
    stage('Build'){
      steps{
        echo 'Building...' 
      }
    }
    
    stage('Test'){
      step{
        echo 'Testing...' 
      }
    }
    
    stage('Deploy'){
      steps{
        echo 'Deploying...' 
      }
    }
    
  }
}
